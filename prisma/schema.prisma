// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Car {
  id       String   @id @default(nanoid()) @map("_id")
  name    String   
  engine Int
  address String
  latitude String
  longitude String
  brand String
  color String
  kilometers Int
  transmission String
  seats Int
  fuel String
  frontimg String
  backimg String 
  description String
  phone Int
  price Int
  renterId String
  rented Boolean
}